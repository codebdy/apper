{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "~/*": ["src/*"],
      "@rxdrag/plugin-sdk/*": ["src/plugin-sdk/*"],
      "@rxdrag/plugin-sdk": ["src/plugin-sdk"]
    },
    "baseUrl": "src"
  },
  "include": [
    "src",
    "craco.config.js"
  ]
}